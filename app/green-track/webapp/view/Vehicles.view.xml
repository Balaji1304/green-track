<mvc:View
    controllerName="greentrack.controller.Vehicles"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:card="sap.f.cards"
    xmlns:core="sap.ui.core">
    
    <Page title="Transport Resource Management" class="sapUiResponsiveContentPadding">
    	<f:Card class="sapUiMediumMargin" width="300px">
		<f:header>
			<card:Header title="Vehicle1- Vehicle Details" subtitle="Here comes the Subtitle" />
		</f:header>
		<f:content>
			<List
				class="sapUiSmallMarginBottom"
				showSeparators="None"
				items="{
					path: 'products>/productItems'
				}">
				<CustomListItem>
					<HBox alignItems="Center" justifyContent="SpaceBetween">
						<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
							<Title level="H3" text="{products>title}" />
							<Text text="{products>subtitle}" />
						</VBox>
						<ObjectStatus
							class="sapUiTinyMargin sapUiSmallMarginEnd"
							text="{products>revenue}"
							state="{products>statusSchema}" />
					</HBox>
				</CustomListItem>
			</List>
		</f:content>
	</f:Card>

    	<f:Card class="sapUiMediumMargin" width="300px">
		<f:header>
			<card:Header title="Project Cloud Transformation" subtitle="Revenue per Product | EUR" />
		</f:header>
		<f:content>
			<List
				class="sapUiSmallMarginBottom"
				showSeparators="None"
				items="{
					path: 'products>/productItems'
				}">
